{
    "name": "net_bazzline/framework_knowledge_test",
    "type": "framework",
    "description": "php framework for building knowledge test for cli and web application",
    "keywords": ["php", "bazzline", "framework", "knowledge test"],
    "homepage": "https://github.com/stevleibelt/php_framework_knowledge_test",
    "license": "LGPLv3",
    "authors": [
        {
            "name": "stev leibelt",
            "email": "artodeto@arcor.de",
            "homepage": "http://artodeto.bazzline.net",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/console": "2.2.*@dev",
        "net_bazzline/php_symfony_console_command" : "dev-master",
        "net_bazzline/php_component_testcase": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mikey179/vfsStream": "dev-master",
	    "mockery/mockery": "dev-master"
    },
    "bin": [
        "bin/net_bazzline_framework_knowledge_test.php"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Net\\Bazzline\\KnowledgeTest": "source/",
            "Test\\Net\\Bazzline\\KnowledgeTest": "test/"
        }
    }
}
